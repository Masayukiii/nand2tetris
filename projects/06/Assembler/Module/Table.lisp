(defun rom-table ()
  (defparameter *rom-table* (make-hash-table :test #'equal))
  (setf (gethash "R0"      *rom-table*) "0000000000000000")
  (setf (gethash "R1"      *rom-table*) "0000000000000001")
  (setf (gethash "R2"      *rom-table*) "0000000000000010")
  (setf (gethash "R3"      *rom-table*) "0000000000000011")
  (setf (gethash "R4"      *rom-table*) "0000000000000100")
  (setf (gethash "R5"      *rom-table*) "0000000000000101")
  (setf (gethash "R6"      *rom-table*) "0000000000000110")
  (setf (gethash "R7"      *rom-table*) "0000000000000111")
  (setf (gethash "R8"      *rom-table*) "0000000000001000")
  (setf (gethash "R9"      *rom-table*) "0000000000001001")
  (setf (gethash "R10"     *rom-table*) "0000000000001010")
  (setf (gethash "R11"     *rom-table*) "0000000000001011")
  (setf (gethash "R12"     *rom-table*) "0000000000001100")
  (setf (gethash "R13"     *rom-table*) "0000000000001101")
  (setf (gethash "R14"     *rom-table*) "0000000000001110")
  (setf (gethash "R15"     *rom-table*) "0000000000001111")
)

